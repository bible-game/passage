{
  "family": "passage-dev",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::us-east-1:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::us-east-1:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "passage-dev",
      "image": "bible/passage",
      "portMappings": [
        { "containerPort": 8081, "hostPort": 8081, "protocol": "tcp" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/passage-dev",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}