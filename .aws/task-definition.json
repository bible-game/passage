{
  "family": "passage",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "passage",
      "image": "passage",
      "portMappings": [
        {
          "containerPort": 8081,
          "hostPort": 8081,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}