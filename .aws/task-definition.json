{
  "family": "passage",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::970547366462:role/service-role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::970547366462:role/passageTaskRole",
  "containerDefinitions": [
    {
      "name": "passage",
      "image": "bible/passage",
      "portMappings": [
        { "containerPort": 8081, "hostPort": 8081, "protocol": "tcp" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/passage-dev",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "essential": true,
      "environment": [
        {
          "name": "PROFILE",
          "value": "prod"
        }
      ],
      "secrets": [
        {
          "name": "REDIS_HOST",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:970547366462:secret:REDIS_HOST"
        },
        {
          "name": "POSTGRES_USER",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:970547366462:secret:POSTGRES_USER"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:970547366462:secret:POSTGRES_PASSWORD"
        },
        {
          "name": "GH_PACKAGES_USER",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:970547366462:secret:GH_PACKAGES_USER"
        },
        {
          "name": "GH_PACKAGES_TOKEN",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:970547366462:secret:GH_PACKAGES_TOKEN"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}